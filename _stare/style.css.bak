#loading-mask {
		position: absolute;
		left:     0;
		top:      0;
		width:    100%;
		height:   100%;
		z-index:  20000;
		background-color: #D8D8D8;
} 
#loading {
		position: absolute;
		left:     50%;
		top:      50%;
		padding:  2px;
		z-index:  20001;
		height:   auto;
		margin:   -35px 0 0 -30px;
}
#loading .loading-indicator {
		background: url(ikony/loading.gif) no-repeat;
		background-position: 7% 50%;
		background-color:#FFFFFF;
		color:      #555;
		font:       bold 13px tahoma,arial,helvetica;
		padding:    8px 42px;
		margin:     0;
		text-align: center;
		height:     auto;
		border: 1px solid #000000;		
}
