#lightbox {
  	position: absolute;
  	top: 0;
  	left: 0;
  	bottom: 0;
  	right: 0;
  	background-color: #ffffff;
  	opacity: 0.5;
  	display: none;
}

.popupPanel {
		white-space: pre-line;
		font-family: monospace;
  	background-color: #f8f8f8;
  	background-image: url(ikonki/kwiatek.png);
  	background-repeat: no-repeat;
  	background-position: 100% 0%;
  	overflow: auto;
  	display: none;
  	position: absolute;
  	top: 30px;
  	left: 30px;
  	padding: 0px 20px 10px 20px;
  	border: 2px solid #004080;
  	-webkit-border-radius: 4px;
  	-moz-border-radius: 4px;
  	border-radius: 4px;
}

#grafika {
		display: block;
		top: 0px;
		left: 0px;
		padding: 0;
		height: 100%;
		width: 100%;
		margin: 0;
		background-position: 100% 100%;
}

.srcTitle {
		font-family:sans-serif;
		margin: 5px;
}

.closeIcon {
  	width: 11px;
  	height: 11px;
  	position: absolute;
  	right: 4px;
  	top: 4px;
  	cursor: pointer;
}
	
.guzikwokienku{
   	font-family:sans-serif;
   	background-color:#FFFFFF;
   	border-style:solid;
   	border-color:#004080;
}

.guziczki {
		margin: 5px;
		z-index: 0;
		position: absolute;
		cursor: pointer;
		left: 60px;
		bottom: 0px;
}

.guzik {
		direction: ltr; 
		overflow: hidden; 
		text-align: center; 
		position: relative; 
		color: white; 
		font-family: Arial,sans-serif; 
		-moz-user-select: none; 
		font-size: 12px; 
		background: rgb(109, 138, 204); 
		line-height: 160%; 
		padding: 0pt 6px; 
		border-radius: 2px 0pt 0pt 2px; 
		box-shadow: 2px 2px 3px rgba(0, 0, 0, 0.35); 
		border: 1px solid rgb(103, 138, 199); 
		font-weight: bold; 
		min-width: 33px;	
}

.guzik:hover {
		border-radius: 2px 0pt 0pt 2px; 
		border: 1px solid rgb(0, 0, 0); 
}

.guzik:active {
		color: black; 
	  background: rgb(243, 243, 243);  
		border-radius: 0pt 2px 2px 0pt;
		border: 1px solid rgb(169, 187, 223); 
		min-width: 40px;
}

.guzikKlik {
		direction: ltr; 
		overflow: hidden; 
		text-align: center; 
		position: relative;
		color: black; 
		font-family: Arial,sans-serif;
		-moz-user-select: none;
		font-size: 12px;
	  background: rgb(243, 243, 243); 
		line-height: 160%;
	  padding: 0pt 6px; 
		border-radius: 0pt 2px 2px 0pt;
		box-shadow: 2px 2px 3px rgba(0, 0, 0, 0.35);
		border: 1px solid rgb(169, 187, 223); 
	  font-weight: bold; 
		min-width: 40px;
}

.guzikKlik:hover {
		border-radius: 2px 2pt 2pt 2px; 
		border: 1px solid rgb(0, 0, 0); 
}

.stopka {
		position: absolute;
		bottom: 0;
		right: 0;
		font-family: Arial,sans-serif;
		font-size: 10px;
		color: #C0C0C0;
}